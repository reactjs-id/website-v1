{
  "name": "gatsby-starter-typescript-plus",
  "description": "A starter kit for TypeScript-based Gatsby projects with sensible defaults.",
  "version": "1.0.0",
  "private": true,
  "author": "Resi Respati <resir014@gmail.com>",
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "build": "gatsby build",
    "clean": "rimraf public",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public",
    "dev": "gatsby develop",
    "format": "npm run format:gatsby && npm run format:src",
    "format:gatsby": "prettier --write \"gatsby-*.{js,jsx}\"",
    "format:src": "prettier --write \"src/**/*.{ts,tsx,md}\"",
    "lint": "npm run lint:gatsby && npm run lint:src",
    "lint:gatsby": "eslint \"gatsby-*.{js,jsx}\"",
    "lint:src": "eslint \"src/**/*.{ts,tsx}\"",
    "start": "npm run dev",
    "test": "npm run type-check && npm run lint",
    "type-check": "tsc"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "dependencies": {
    "@emotion/core": "^10.0.27",
    "@emotion/styled": "^10.0.23",
    "babel-plugin-styled-components": "^1.10.6",
    "classnames": "^2.2.6",
    "gatsby": "^2.23.14",
    "gatsby-image": "^2.2.37",
    "gatsby-plugin-canonical-urls": "^2.3.8",
    "gatsby-plugin-emotion": "^4.1.18",
    "gatsby-plugin-layout": "^1.1.18",
    "gatsby-plugin-manifest": "^2.2.34",
    "gatsby-plugin-netlify": "^2.1.30",
    "gatsby-plugin-netlify-cache": "^1.2.0",
    "gatsby-plugin-offline": "^3.2.0",
    "gatsby-plugin-react-helmet": "^3.1.23",
    "gatsby-plugin-sharp": "^2.6.0",
    "gatsby-plugin-typescript": "^2.4.16",
    "gatsby-remark-copy-linked-files": "^2.1.33",
    "gatsby-remark-images": "^3.1.49",
    "gatsby-remark-prismjs": "^3.5.8",
    "gatsby-remark-responsive-iframe": "^2.2.30",
    "gatsby-remark-smartypants": "^2.1.19",
    "gatsby-source-filesystem": "^2.1.43",
    "gatsby-transformer-json": "^2.2.27",
    "gatsby-transformer-remark": "^2.6.42",
    "gatsby-transformer-sharp": "^2.3.9",
    "htmr": "^0.8.6",
    "modern-normalize": "^0.6.0",
    "normalize.css": "^8.0.1",
    "polished": "^3.4.2",
    "prism-themes": "^1.4.0",
    "prismjs": "^1.21.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^5.2.1",
    "react-scroll": "^1.7.16",
    "react-transition-group": "^4.3.0",
    "typescript": "^3.9.3"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.9",
    "@types/node": "^14.0.27",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.4",
    "@types/react-helmet": "^6.0.0",
    "@types/react-scroll": "^1.5.5",
    "@types/react-transition-group": "^4.2.3",
    "@typescript-eslint/eslint-plugin": "^2.20.0",
    "@typescript-eslint/parser": "^2.20.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "~18.0.1",
    "eslint-config-prettier": "^6.10.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "gh-pages": "^2.1.1",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.0"
  },
  "resolutions": {
    "sharp": "0.22.1"
  }
}
